pico-8 cartridge // http://www.pico-8.com
version 16
__lua__
function _init()
	p={x=64,y=110,s=16}
end

function _update60()
	if btn(0) then
		p.x=p.x-1
		p.s=17
	elseif btn(1) then
		p.x=p.x+1
		p.s=18
	else p.s=16
	end
end

function _draw()
	cls()
	spr(p.s,p.x,p.y)
	spr(19,p.x,p.y+8)
end
