pico-8 cartridge // http://www.pico-8.com
version 42
__lua__

local __hid__="20:0:22:2:0:01.01.01;01.16.16;01.16.0c,14:7:14:10:1:14.14.14.14;14.14.14.14;14.14.14.14;0d.14.14.14"

local hidden={}
local rooms=split(__hid__)
for room in all(rooms) do 
    local cells={}
    local ox,oy,tx,ty,d,s=unpack(split(room,':'))   
    local rows=split(s,';')
    for row in all(rows) do 
        add(cells,split(row,'.'))
    end
    add(hidden,{ox=ox,oy=oy,tx=tx,ty=ty,dir=d,cells=cells,visible=false})
end

print(hidden[1].ox)

for y,row in ipairs(hidden[1].cells) do 
    for x,sprite in ipairs(row) do 
        print(x..', '..y..' : '..sprite)
    end
end

