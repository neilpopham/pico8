pixel={
    create=function(self,x,y,dir)
        local o=setmetatable(
            {
                x=x,
                y=y,
                dir=dir
            },
            self
        )
        self.__index=self
        return o
    end,
    tile=function(p)
        return flr(p/8)
    end
} setmetatable(pixel,{__index=_ENV})

frog={
    create=function(self,x,y,dir)
        local o=pixel.create(self,x,y,dir)
        o.c=3
        o.reset(self)
        return o
    end,
    reset=function(_ENV)
        printh('###################')
        t=0
        stage=0
        r=range(12,16)
    end,
    reverse=function(_ENV)
        dir=dir==1 and -1 or 1
    end,
    update=function(_ENV)
        if stage==0 then return end
        local dx,dy,cx,cy,tx,ty,ti
        if stage==1 then
            dx=2*dir
            dy=-1
            if t==0 then sfx(0) end
            -- check whether there is a gap to jump before trying
            -- if t==0 then
            --     tx=flr((x+(dir==1 and 8 or -8))/8)
            --     ty=flr(y/8)
            --     tile=mget(tx,ty)
            --     if fget(tile,0) then self:turn() end
            -- end
        else
            dx=stage==2 and dir or 0
            dy=1
        end
        cx=x+(2*dir)
        cy=y+(stage==1 and -2 or 2)
        tx=tile(cx+dx)
        ty=tile(cy+dy)
        ti=mget(tx,ty)
        if fget(ti,0) then
            printh('HIT')
            ti=mget(tile(cx),ty)
            if fget(ti,0) then
                printh('V'..stage)
                if stage==1 then
                    y=ty*8+8
                else
                    y=ty*8-1
                    cy=y
                    reset(_ENV)
                end
                printh('Y'..y)
            end
            ti=mget(tx,tile(cy))
            if fget(ti,0) then
                printh('H'..stage)
                printh('X'..x)
                x=tx*8+(dir==1 and -3 or 10)
                printh('X'..x)
                if stage==1 and t<6 then dir=dir==1 and -1 or 1 end -- reverse() end
            end
            if stage>0 then stage=3 end
            dx=0
            dy=0
        end
        x+=dx
        y+=dy
        x=x%128
        if stage==1 then
            t+=1
            if t==r then
                stage=2
            end
        end
    end,
    draw=function(_ENV)
        local ex=x+2*dir
        if stage==0 then
            line(x,y,x+1,y,c)
            pset(x+(dir==1 and 1 or 0),y-1,c)
        elseif stage==1 then
            line(x,y,ex,y-2,c)
        else
            line(x,y,ex,y+2,c)
        end
    end
} setmetatable(frog,{__index=pixel})

