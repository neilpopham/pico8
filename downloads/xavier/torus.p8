version 8
__lua__

-- torus

torus_rings = 12
torus_sides = 12

torus_camera = {0, 0, -2.5, 64}
torus_vertices = {}
torus_transformed_vertices = {}


function torus_init(rings, sides, outer_radius, inner_radius)
 for u=0,rings-1 do
  local rho = u / rings
  for v=0,sides-1 do
   local theta = v / sides
   local x = (outer_radius + inner_radius * cos(theta)) * cos(rho)
   local y = (outer_radius + inner_radius * cos(theta)) * sin(rho)
   local z = inner_radius * sin(theta)
   add(torus_vertices, {x, y, z})
  end
 end
end


function project(vertex, cam)
 local mult = cam[4]
 local d = (vertex[3]-cam[3])
 local x = (vertex[1]-cam[1]) * mult / d + 64
 local y = -(vertex[2]-cam[2]) * mult / d + 64
 return x, y
end


function torus_update(frame)
 local rx = frame * 0.001
 local ry = frame * 0.01
 local rz = frame * 0.0001
 local s  = 1 --+ 0.5 * sin(frame * 0.01)
 torus_transformed_vertices = transform_vertices(torus_vertices, rx, ry, rz, s, s, s)
end

function transform_vertices(vertices, rx, ry, rz, sx, sy, sz)
  transformed_vertices = {}
  for v in all(vertices) do
    nv = {}
    nv[1] = v[1] * sx
    nv[2] = v[2] * sy
    nv[3] = v[3] * sz
    nv = rotate_vertex(nv, 1, rx)
    nv = rotate_vertex(nv, 2, ry)
    nv = rotate_vertex(nv, 3, rz)
    nv[3] -= 20 -- todo add translation param
    add(transformed_vertices, nv)
  end
  return transformed_vertices
end

function rotate_vertex(v,a,r)
  -- figure out which axis we're rotating on
  if a==1 then
    x,y,z = 3,2,1
  elseif a==2 then
    x,y,z = 1,3,2
  elseif a==3 then
    x,y,z = 1,2,3
  end
  _x = cos(r)*(v[x]) - sin(r) * (v[y]) -- calculate the new x location
  _y = sin(r)*(v[x]) + cos(r) * (v[y]) -- calculate the new y location
  nv = {}
  nv[x] = _x
  nv[y] = _y
  nv[z] = v[z]
  return nv
end

function torus_draw()
 torus_draw_rings(torus_rings, torus_sides)
end

function torus_draw_dots()
 for vertex in all(transformed_vertices) do
  local x, y = project(vertex, torus_camera)
  if vertex[3] < 0 then
   pset(x, y, 6)
  else
   pset(x, y, 7)
  end
 end
end

function torus_draw_rings(rings, sides)
 palette = {8, 9, 10, 7, 10, 9, 8}
 for ring=0,rings-1 do
  local base = 1 + ring * sides
  for s=0,sides-1 do
   local x1, y1 = project(transformed_vertices[base+s], torus_camera)
   local x2, y2 = project(transformed_vertices[base+(s+1)%sides], torus_camera)
   line(x1, y1, x2, y2, palette[1+(ring+s) % #palette])
  end
 end
end

-- bg

function bg_update(frame)
 bg_ang_X = frame * 0.0095
 bg_amp_X = 8
 bg_ang_y = frame * 0.0090
 bg_amp_y = 8
end

function bg_draw()
 cls(2)

 for y=0,7 do
  for x=0,7 do
   local u = 8 + x*16 + bg_amp_X*sin(bg_ang_X+x/16)
   local v = 8 + y*16 + bg_amp_y*sin(bg_ang_y+y/16)
   --pset(u, v, 13)
   spr(16+((v-u)/4)%5, u, v)
  end
 end
 rect(1, 1, 126, 126, 5)
 rect(3, 3, 124, 124, 0)
 print("45!p!rn3 <<<<<<<<<<<<<<<<<<<<", 7, 6, 14)
 print(">>>>>>>>>>>>>>>>>>>>>>>> 20!7", 7, 117, 14)
end

-- main

function _init()
 frame = 0
 torus_init(torus_rings, torus_sides, 10, 4)
end


function _update()
 bg_update(frame)
 torus_update(frame)
 frame += 1
end

function _draw()
 bg_draw()
 torus_draw()
end


