pico-8 cartridge // http://www.pico-8.com
version 8
__lua__

-- torus

torus_rings = 12
torus_sides = 12

torus_camera = {0, 0, -2.5, 64}
torus_vertices = {}
torus_transformed_vertices = {}



function torus_init(rings, sides, outer_radius, inner_radius)
 local rho = 0
 local rho_incr = 1 / (rings * sides)
 for u=0,rings-1 do
  for v=0,sides-1 do
   local theta = v / sides
   local x = (outer_radius + inner_radius * cos(theta)) * cos(rho)
   local y = (outer_radius + inner_radius * cos(theta)) * sin(rho)
   local z = inner_radius * sin(theta)
   add(torus_vertices, {x, y, z})
   rho += rho_incr
  end
 end
end



function project(vertex, cam)
 local mult = cam[4]
 local d = (vertex[3]-cam[3])
 local x = (vertex[1]-cam[1]) * mult / d + 64
 local y = -(vertex[2]-cam[2]) * mult / d + 64
 return x, y
end


function torus_update(frame)
 local rx = frame * 0.001
 local ry = frame * 0.01
 local rz = 0 --frame * 0.0001
 local s  = 1 --+ 0.5 * sin(frame * 0.01)
 torus_transformed_vertices = transform_vertices(torus_vertices, rx, ry, rz, s, s, s)
end

function transform_vertices(vertices, rx, ry, rz, sx, sy, sz)
  transformed_vertices = {}
  for v in all(vertices) do
    nv = {}
    nv[1] = v[1] * sx
    nv[2] = v[2] * sy
    nv[3] = v[3] * sz
    nv = rotate_vertex(nv, 1, rx)
    nv = rotate_vertex(nv, 2, ry)
    nv = rotate_vertex(nv, 3, rz)
    nv[3] -= 20 -- todo add translation param
    add(transformed_vertices, nv)
  end
  return transformed_vertices
end

function rotate_vertex(v,a,r)
  -- figure out which axis we're rotating on
  if a==1 then
    x,y,z = 3,2,1
  elseif a==2 then
    x,y,z = 1,3,2
  elseif a==3 then
    x,y,z = 1,2,3
  end
  _x = cos(r)*(v[x]) - sin(r) * (v[y]) -- calculate the new x location
  _y = sin(r)*(v[x]) + cos(r) * (v[y]) -- calculate the new y location
  nv = {}
  nv[x] = _x
  nv[y] = _y
  nv[z] = v[z]
  return nv
end

function torus_draw()
 torus_draw_lines()
end

function torus_draw_dots()
 for vertex in all(transformed_vertices) do
  local x, y = project(vertex, torus_camera)
  pset(x, y, 0)
 end
end

function torus_draw_lines()
 local palette = {7, 8}
 local n = #transformed_vertices
 if frame < n then
  n = max(1, frame)
 end
 for i = 1,n do
  local x1, y1 = project(transformed_vertices[i], torus_camera)
  local i += 1
  if i > #transformed_vertices then
   i = 1
  end
  local x2, y2 = project(transformed_vertices[i], torus_camera)
  line(x1, y1, x2, y2, palette[1+(i % #palette)])
 end
end

-- bg

function bg_update(frame)
 bg_angle = frame * 0.025
 bg_ampl  = 10 + 8 * sin(frame * 0.025)
 bg_base  = 30 + 16 * sin(frame * 0.01)
end

function bg_draw()
 cls(0x0c)
 col = 0xcd

 for y=0,127 do
  local w = bg_base + bg_ampl * sin(bg_angle + y / 128)
  local offset = y * 64 + shr(64-w, 1)
  memset(0X6000+offset, col, abs(w))
  col = swap_nibbles(col)
 end

end

function swap_nibbles(x)
 local lo = shr(band(col, 0xf0), 4)
 local hi = shl(band(col, 0x0f), 4)
 return bor(hi, lo)
end

-- bubbles

function bubbles_init()
 bubbles = {}
 for i=1,7 do
  local bubble = {
   x=10+rnd(100),
   y=128,
   speed=rnd(3),
   exc=rnd(3)
  }
  add(bubbles, bubble)
 end

end

function bubbles_update(frame)
 for bubble in all(bubbles) do
  if bubble.y > -8 then
   bubble.y -= bubble.speed
   bubble.x += sin(frame*0.01*bubble.exc)
  else
   bubble.x = 10+rnd(100)
   bubble.y = 128
   bubble.speed=rnd(3)
   bubble.exc=rnd(3)
  end
 end
end

function bubbles_draw()
 for bubble in all(bubbles) do
  spr(2, bubble.x, bubble.y)
 end
end

-- frame

function frame_draw()
 -- rounded corners
 spr(1, 0, 0, 1, 1, false, false)
 spr(1, 120, 0, 1, 1, true, false)
 spr(1, 0, 120, 1, 1, false, true)
 spr(1, 120, 120, 1, 1, true, true)
end

-- main

function _init()
 frame = 0
 torus_init(torus_rings, torus_sides, 10, 4)
 bubbles_init()
end


function _update()
 bg_update(frame)
 torus_update(frame)
 bubbles_update(frame)
 frame += 1
end

function _draw()
 palt(0, false)
 palt(11, true)
 bg_draw()
 torus_draw()
 spr(16,0,48,16,4)
 bubbles_draw()
 frame_draw()
end


